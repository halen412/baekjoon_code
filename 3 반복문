# 10950 A + B - 3
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	int a, b, i;
	scanf("%d", &i);
	int sum[100];
	for (int j = 0; j < i; j++) {
		scanf("%d %d", &a, &b);
		sum[j] = a + b;
	}
	for (int j = 0; j < i; j++) {
		printf("%d\n", sum[j]);
	}

	return 0;
}

