# 25314	코딩은 체육과목 입니다
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	int n;
	scanf("%d", &n);
	n /= 4;
	for (int i = 1; i <= n; i++) {
		printf("long ");
	}
	printf("int");

	return 0;
}

