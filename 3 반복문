# 8393 합
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	int n, sum = 0;
	scanf("%d", &n);
	for (int i = 0; i <= n; i++) {
		sum += i;
	}
	printf("%d", sum);

	return 0;
}

------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------------
# 25304 영수증
#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	int x, n, a, b, sum = 0;
	scanf("%d\n%d", &x, &n);
	for (int i = 1; i <= n; i++) {
		scanf("%d %d", &a, &b);
		sum += (a * b);
	}
	if (sum == x) {
		printf("Yes");
	}
	else {
		printf("No");
	}

	return 0;
}

